generator client {
  provider     = "prisma-client"
  output       = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          Int      @id @default(autoincrement())
  telegram_id Int   @unique
  email       String?   @unique
  name        String?
  username    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
